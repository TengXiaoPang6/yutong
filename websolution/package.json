  "scripts": {
    "yutong": "node -e console.log('====常用命令说明bootstrap:安装依赖包dev:启动开发环境devserver:启动开发环境，不安装依赖====')",
    "yutongdev": "node -e console.log('====常用命令说明build:prod:生产环境打包build:sit:测试环境打包test:进行语法检查并尝试修复格式错误====')",
    "bootstrap": "npm install --registry=http://10.66.196.55:7000",
    "dev": "npm run bootstrap && webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "devserver": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "build:prod": "npm run build:theme && cross-env NODE_ENV=production env_config=prod node build/build.js",
    "build:sit": "npm run build:theme && cross-env NODE_ENV=production env_config=sit npm_config_preview=true npm_config_report=true node build/build.js",
    "build:theme": "gulp build --gulpfile gulpfile.js",
    "lint": "eslint --ext .js,.vue src",
    "test": "npm run lint -- --fix"
  },
